#! /bin/bash
#set key inside left top vertical Right noreverse enhanced autotitles box linetype -1 linewidth 1.000
set title "Step time average over all reruns(Y) vs Iteration values(X)"
set style fill solid 1.00 border lt -1
set boxwidth 0.5 absolute
set xlabel "Iteration values"
set ylabel "Average step time (in ms)"
set yrange [-5:250]
set datafile separator ","
plot '../data/temp1.csv' using 1:2 with boxes title "Step time","" using 1:6 with lines title "Loop time"
min_y = GPVAL_DATA_Y_MIN
max_y = GPVAL_DATA_Y_MAX
set terminal png
set output "../plots/g09_plot01.png"
plot '../data/temp1.csv' using 1:2 with boxes title "Step time","" using 1:6 with lines title "Loop time",min_y ,max_y
