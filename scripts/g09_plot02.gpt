#! /bin/bash
set terminal png
set output "../plots/g09_plot02.png"
#set key inside left top vertical Right noreverse enhanced autotitles box linetype -1 linewidth 1.000
set title "Step time average over all reruns(Y) vs Iteration values(X)"
set style fill solid 1.00 border lt -1
set boxwidth 0.5 absolute
set xlabel "Iteration values"
set ylabel "Average step time (in ms)"
set datafile separator ","
plot '../data/temp1.csv' using 1:2 with lines title "Step time","" using 1:3 with lines title "Collision time","" using 1:4 with lines title "Velocity time","" using 1:7 with lines title "Position time","" using 1:($3 + $4 + $7) with lines title "Sum of collisions,velocity,position"

